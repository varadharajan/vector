float square(float x) {
 return x * x;
}

int vec_main()
{
    float inputs[2048];
    int check;

    for (i in 0:2048)
        inputs[i] = float(i);

    squares := @map(square, inputs);

    for (i in 0:2048) {
        check = int(squares[i]);
        if (check != i * i) {
            printf("incorrect\n");
            return -1;
        }
    }

    printf("correct\n");
    return 0;
}
