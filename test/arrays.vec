int vec_main()
{
    int arr[2, 2];
    int i;
    int j;

    arr[0, 0] = 0;
    arr[0, 1] = 1;
    arr[1, 0] = 2;
    arr[1, 1] = 3;

    i = 0;
    while (i < 2) {
        j = 0;
        while (j < 2) {
            printf("%d\n", arr[i, j]);
            j++;
        }
        i++;
    }

    return 0;
}
